{
    "version":2,
    "env":{
        "MONGODB_URI": "@mongodb_uri"
    },
    "builds":[
        {
            "src":"src/main.ts",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"src/main.ts",
            "methods":[
                "GET",
                "POST",
                "PATCH",
                "DELETE"
            ]
        }
    ]
}